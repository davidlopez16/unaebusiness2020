<p id="notice"><%= notice %></p>

<h1>Capacidads</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Nivel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @capacidads.each do |capacidad| %>
      <tr>
        <td><%= capacidad.tipo %></td>
        <td><%= capacidad.nivel %></td>
        <td><%= link_to 'Show', capacidad %></td>
        <td><%= link_to 'Edit', edit_capacidad_path(capacidad) %></td>
        <td><%= link_to 'Destroy', capacidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Capacidad', new_capacidad_path %>
